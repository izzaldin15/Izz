        ];

        const prizes = [0, 100, 200, 300, 500, 1000, 2000, 4000, 8000, 16000, 32000, 64000, 125000, 250000, 500000, 1000000];
        const safeLevels = [5, 10];
        let gameQuestions = [];
        let currentQuestion = 0;
        let lifelineUsed = { fifty: false, audience: false, friend: false };

        function startGame() {
            gameQuestions = [...allQuestions]
                .sort(() => 0.5 - Math.random())
                .slice(0, 15);
            
            currentQuestion = 0;
            lifelineUsed = { fifty: false, audience: false, friend: false };
            document.querySelector('button').style.display = 'none';
            document.getElementById('game').style.display = 'block';
            document.querySelectorAll('.lifeline').forEach(el => el.classList.remove('used'));
            nextQuestion();
        }

        function nextQuestion() {
            if (currentQuestion >= gameQuestions.length) {
                document.getElementById('result').innerHTML = `Ù…Ø¨Ø±ÙˆÙƒ! Ø±Ø¨Ø­Øª Ø§Ù„Ù…Ù„ÙŠÙˆÙ† ğŸ’°ğŸ‰`;
                return;
            }
            updateMoneyTree();
            document.getElementById('question').textContent = `Ø§Ù„Ø³Ø¤Ø§Ù„ ${currentQuestion + 1}: ${gameQuestions[currentQuestion].q}`;
            const opts = document.getElementById('options');
            opts.innerHTML = '';
            gameQuestions[currentQuestion].a.forEach((ans, i) => {
                const div = document.createElement('div');
                div.className = 'option';
                div.textContent = `${String.fromCharCode(1571 + i)}) ${ans}`;
                div.onclick = () => selectAnswer(i);
                opts.appendChild(div);
            });
            document.getElementById('result').innerHTML = '';
            document.querySelectorAll('.option').forEach(opt => opt.style.display = 'block');
        }

        function selectAnswer(selected) {
            const correct = gameQuestions[currentQuestion].c;
            const options = document.querySelectorAll('.option');
            options[correct].classList.add('correct');
            if (selected !== correct) {
                options[selected].classList.add('wrong');
                let safe = 0;
                if (currentQuestion >= safeLevels[1]) safe = prizes[safeLevels[1]];
                else if (currentQuestion >= safeLevels[0]) safe = prizes[safeLevels[0]];
                setTimeout(() => {
                    document.getElementById('result').innerHTML = `Ø®Ø·Ø£! Ø±Ø¨Ø­Øª ${safe.toLocaleString()} ÙÙ‚Ø· ğŸ˜<br><button onclick="location.reload()">Ø§Ù„Ø¹Ø¨ Ù…Ø±Ø© Ø£Ø®Ø±Ù‰</button>`;
                }, 3000);
            } else {
                currentQuestion++;
                document.getElementById('result').innerHTML = 'ØµØ­ÙŠØ­! ğŸ‘';
                setTimeout(nextQuestion, 3000);
            }
        }

        function updateMoneyTree() {
            const tree = document.getElementById('money-tree').children;
            for (let i = 0; i < tree.length; i++) {
                tree[i].classList.remove('current', 'safe');
            }
            for (let i = 0; i < safeLevels.length; i++) {
                if (currentQuestion > safeLevels[i]) tree[14 - safeLevels[i]].classList.add('safe');
            }
            if (currentQuestion < 15) {
                tree[14 - currentQuestion].classList.add('current');
            }
        }

        function use5050() {
            if (lifelineUsed.fifty) return;
            lifelineUsed.fifty = true;
            document.getElementById('fifty').classList.add('used');
            const correct = gameQuestions[currentQuestion].c;
            const options = document.querySelectorAll('.option');
            let removed = 0;
            for (let i = 0; i < 4 && removed < 2; i++) {
                if (i !== correct && options[i].style.display !== 'none') {
                    options[i].style.display = 'none';
                    removed++;
                }
            }
        }

        function useAudience() {
            if (lifelineUsed.audience) return;
            lifelineUsed.audience = true;
            document.getElementById('audience').classList.add('used');
            const correct = gameQuestions[currentQuestion].c;
            let percentages = [5, 5, 5, 5];
            percentages[correct] = 75;
            let remaining = 20;
            for (let i = 0; i < 4; i++) {
                if (i !== correct) {
                    percentages[i] = Math.floor(Math.random() * remaining) + 5;
                    remaining -= percentages[i];
                }
            }
            percentages[correct] += remaining;
            alert(`Ø§Ø³ØªØ·Ù„Ø§Ø¹ Ø§Ù„Ø¬Ù…Ù‡ÙˆØ±:\nØ£: ${percentages[0]}%\nØ¨: ${percentages[1]}%\nØ¬: ${percentages[2]}%\nØ¯: ${percentages[3]}%`);
        }

        function useFriend() {
            if (lifelineUsed.friend) return;
            lifelineUsed.friend = true;
            document.getElementById('friend').classList.add('used');
            const correctAns = gameQuestions[currentQuestion].a[gameQuestions[currentQuestion].c];
            const confidence = Math.random() > 0.2 ? "Ù…ØªØ£ÙƒØ¯ Ø¬Ø¯Ù‹Ø§" : "Ø£Ø¹ØªÙ‚Ø¯";
            alert(`ØµØ¯ÙŠÙ‚Ùƒ ÙŠÙ‚ÙˆÙ„: \( {confidence} Ø£Ù† Ø§Ù„Ø¥Ø¬Ø§Ø¨Ø© Ù‡ÙŠ " \){correctAns}"`);
        }
    </script>
